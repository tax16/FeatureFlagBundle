{
        "type": "symfony-bundle",
        "name": "tax16/feature-flag",
        "description": "Feature flag bundle",
        "suggest": {
                "doctrine/orm": "Required if using Doctrine for data storage",
                "doctrine/dbal": "Needed for low-level database abstraction",
                "doctrine/doctrine-bundle": "Needed to integrate Doctrine with Symfony",
                "doctrine/doctrine-migrations-bundle": "Needed for database migrations"
        },
        "require": {
                "php": ">=8.2",
                "ext-ctype": "*",
                "ext-iconv": "*",
                "laminas/laminas-code": "^4.0",
                "friendsofphp/proxy-manager-lts": "^1.0",
                "symfony/console": "^6.4 || ^7.0",
                "symfony/dotenv": "^6.4 || ^7.0",
                "symfony/flex": "^2",
                "symfony/framework-bundle": "^6.4 || ^7.0",
                "symfony/runtime": "^6.4 || ^7.0",
                "symfony/yaml": "^6.4 || ^7.0",
                "symfony/form": "^6.4 || ^7.0"
        },
        "require-dev": {
                "codeception/codeception": "*",
                "codeception/module-asserts": "*",
                "codeception/module-phpbrowser": "*",
                "codeception/module-symfony": "*",
                "friendsofphp/php-cs-fixer": "*",
                "phpro/grumphp": "*",
                "phpstan/phpstan": "*",
                "phpstan/phpstan-symfony": "*"
        },
        "autoload": {
            "psr-4": {
                "Tax16\\FeatureFlagBundle\\": "src/"
            }
        },
        "autoload-dev": {
            "psr-4": {
                "App\\Tests\\": "tests/"
            }
        },
        "license": "MIT",
        "authors": [
            {
                "name": "Tahiana RAKOTONIRINA",
                "email": "tahiana0@gmail.com"
            }
        ],
        "config": {
                "allow-plugins": {
                        "phpro/grumphp": true,
                        "symfony/flex": true,
                        "symfony/runtime": true
                }
        }
}
